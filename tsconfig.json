{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@config": ["src/config"],
      "@config/*": ["src/config/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@router": ["src/router"],
      "@router/*": ["src/router/*"],
      "@com": ["src/components"],
      "@com/*": ["src/components/*"],
      "@img": ["src/img"],
      "@img/*": ["src/img/*"],
      "@axios": ["src/axios"],
      "@axios/*": ["src/axios/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@types": ["src/types"],
      "@types/*": ["src/types/*"]
    },
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true
  },
  "include": ["src", "global.d.ts"],
  "exclude": ["node_modules"]
}
